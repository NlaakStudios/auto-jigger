.headerDIV{display:block;text-align:center}
.headerBigGold{font-size:170%;color:gold;padding-right:.5em}
.headerSmall{font-size:85%;color:#D2691E;text-transform:uppercase}

.dropup-menu{bottom:100%;top:auto}
.navbar .dropup-menu:before{
	border-bottom:none;
	border-left:7px solid transparent;
	border-right:7px solid transparent;
	border-top:7px solid black;
	left:7px;
	top:100%
}
.navbar .dropup-menu:after{border-bottom:none;border-left:6px solid transparent;border-right:6px solid transparent;border-top:6px solid #fff;top:100%;left:6px}
.up{border-left:4px solid transparent;border-right:4px solid transparent;border-bottom:4px solid #000;border-top:none}

.navbar-fluid{width:100%}

.bs-callout{margin:20px 0;padding:20px;border-left:3px solid #EEE}
ul .bs-callout{margin-left:20px}
.bs-callout-danger{background-color:#241111;border-color:#D9534F}
.bs-callout-warning{background-color:#20190F;border-color:#F0AD4E}
.bs-callout-primary{background-color:#0B1216;border-color:#5BC0DE}
.bs-callout-success{background-color:#0C1A0C;border-color:#5CB85C}

.vanish{position:absolute;display:none;visibility:hidden;top:-10000}
.appear{position:inherit;display:block;visibility:visible;top:inherit}
