@media only screen and (-webkit-min-device-pixel-ratio:1){.dpi:before{content:'1'}.dpi:before{content:'96'}.dpiScale{zoom:1.0}}
@media only screen and (-webkit-min-device-pixel-ratio:1.3){.dpi:before{content:'1.3'}.dpi:before{content:'124'}.dpiScale{zoom:0.75}}
@media only screen and (-webkit-min-device-pixel-ratio:1.5){.dpi:before{content:'1.5'}.dpi:before{content:'144'}.dpiScale{zoom:0.66}}
@media only screen and (-webkit-min-device-pixel-ratio:2){.dpi:before{content:'2'}.dpi:before{content:'192'}.dpiScale{zoom:0.50}.isMobile{content:'1';}}
@media only screen and (-webkit-min-device-pixel-ratio:3){.dpi:before{content:'3'}.dpi:before{content:'288'}.dpiScale{zoom:0.33}.isMobile{content:'1';}}
@media only screen and (-webkit-min-device-pixel-ratio:4){.dpi:before{content:'4'}.dpi:before{content:'384'}.dpiScale{zoom:0.25}.isMobile{content:'1';}}

/* Scrollbars */
::-webkit-scrollbar {width: .5em}
/* Track */
::-webkit-scrollbar-track {
	border-radius: .5em;
	background: rgb(0,128,0);
	-webkit-box-shadow: inset 0 0 .25em rgba(0,128,0,0.3);
	   -moz-box-shadow: inset 0 0 .25em rgba(0,128,0,0.3);
	        box-shadow: inset 0 0 .25em rgba(0,128,0,0.3);
}
/* Handle */
::-webkit-scrollbar-thumb {
	background: rgb(0,96,0);
	border-radius: .5em;
	
	-webkit-box-shadow: inset 0 0 .25em rgba(0,128,0,0.5);
	   -moz-box-shadow: inset 0 0 .25em rgba(0,128,0,0.5);
	        box-shadow: inset 0 0 .25em rgba(0,128,0,0.5);
}
::-webkit-scrollbar-thumb:window-inactive {
	background: rgb(0,64,0);
	background: rgba(0,128,0,0.4)
}	

